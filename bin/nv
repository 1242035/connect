#!/usr/bin/env node

/**
 * Module dependencies
 */
var cwd = process.cwd()
  , path = require('path')
  ;
var cli = require(path.join(__dirname, '../lib/cli'));


/**
 * Parse options
 */

var argv = require('optimist')
  .usage('Usage: $0 command')
  .argv
  ;

/**
 * Run the command
 */

cli.run(argv);
